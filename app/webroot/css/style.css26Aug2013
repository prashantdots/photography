* { padding:0; margin:0; outline:0; }
body { font-family: Verdana, sans-serif; font-size:11px; line-height:1.2; color:#000; background: url("../img/content-background.jpg") repeat scroll center center transparent; overflow-x: hidden; }
a { color: #58585A; text-decoration: none; transition: color 0.5s linear 0s; -moz-transition: color 0.5s linear 0s; -webkit-transition: color 0.5s linear 0s; -o-transition: color 0.5s linear 0s; }
a:hover{ color: #000; }
a:focus { color: #000; }
input[type="text"] { height:24px; width:270px; }
input[type="password"] { height:24px; width:270px; }
input, textarea { font-family: Verdana, sans-serif; font-size:13px; }
textarea { overflow:hidden; }
select { height: 26px; width: 200px; font-family: Verdana, sans-serif; font-size:13px; }
.field { border:solid 1px #d3cfc7; background:#fff; padding:5px; }
.small-field { border:solid 1px #d3cfc7; background:#fff; padding:3px 1px; font-size:11px; }
.checkbox { width:13px; height:13px; vertical-align:top; position:relative; top:2px; }
.button { cursor:pointer; background: rgb(235,94,186); background: url(data:image/svg+xml;base64,); background: -moz-linear-gradient(top, rgba(235, 94, 186, 1) 0%, rgba(230, 21, 151, 1) 100%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(235, 94, 186, 1)), color-stop(100%, rgba(230, 21, 151, 1))); background: -webkit-linear-gradient(top, rgba(235, 94, 186, 1) 0%, rgba(230, 21, 151, 1) 100%); background: -o-linear-gradient(top, rgba(235, 94, 186, 1) 0%, rgba(230, 21, 151, 1) 100%); background: -ms-linear-gradient(top, rgba(235, 94, 186, 1) 0%, rgba(230, 21, 151, 1) 100%); background: linear-gradient(to bottom, rgba(235, 94, 186, 1) 0%, rgba(230, 21, 151, 1) 100%); filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eb5eba', endColorstr='#e61597', GradientType=0 );
text-shadow: 1px 1px 2px #982670; border: 1px solid #E532A0; border-radius: 4px 4px 4px 4px; color: #FFFFFF; text-align: center; font-size: 16px; padding: 4px 25px; box-shadow: 0 0 5px rgba(0, 0, 0, .3); }
.button:hover { background: none repeat scroll 0 0 #E532A0; }
.field:focus { color:#000; border-color:#857b42; }
a img { border:0; }
a { color:#EC008C; text-decoration: underline; cursor:pointer; }
a:hover { color:#8d341f; text-decoration: none; }
.left, .alignleft { float:left; display:inline; }
.right, .alignright { float:right; display:inline; }
.cl { font-size:0; line-height:0; clear:both; display:block; height:0; }
.al { text-align: left; }
.ar { text-align: right; }
.ac { text-align: center !important; }
h1 { font-size:22px; line-height:24px; color:#fff; font-weight: normal; }
h1 a { color:#fff; text-decoration: none; }
h2 { font-size:15px; font-weight: normal; }
h3 { font-size:12px; line-height:14px; font-weight: normal; }
h3 a { text-decoration: none; }
.shell { width:988px; margin:0 auto; }
#header { }
#header h1 { margin:0; padding:12px 0 20px; text-align:center; }
#header h2 { margin:0; padding:0 0 20px; text-align:center; color:#fff; font-size:30px; }
#top-navigation { position:absolute; right:0; top:15px; float:right; white-space:nowrap; color:#fff;}
#top-navigation a { color:#fff;text-decoration:none; }
#top-navigation a:hover { text-decoration:underline; }
#top {   position: relative;}
#navigation {border-top:1px solid #333333; padding:0; }
#navigation ul { list-style-type: none; padding:10px 0px;  text-align:center;}
#navigation ul li { display:inline; padding:0px 20px; border-left:#fff solid 1px;}
#navigation ul li:first-child{border:0 none;}
#navigation ul li a { font-weight:bold; font-size:14px; text-decoration: none; color:#fff;}
#navigation ul li a span { }
#navigation ul li a.active, #navigation ul li a:hover {color:#ef008c;}
#navigation ul li a.active span, #navigation ul li a:hover span {}
#container { padding:0 0 20px 0; min-height:100%; }
.small-nav {     padding: 10px 0;color:#fff;   border-top: 1px solid #333333;}
.small-nav a { }
#main { }
#content { }
.login-box { border-top:#323433 solid 1px;  color:#fff; max-width:534px; font-size:12px; line-height:1.2; margin:0 auto 50px; }
.login-box .login-box-wrapper { width:290px; margin:0 auto; }
.login-box .login-box-head { border-bottom:#323433 solid 1px; display:none; }
.login-box .login-box-head h2 { font-size:24px; padding:10px 0; line-height:1.2; }
.login-box .login-form { padding:15px 0 0; width:290px; margin:0 auto; }
.login-box .login-form label { font-size:12px; display:none; }
.login-box .login-form div.input { margin-bottom:15px; border-top:#323433 solid 1px; }
.login-box .login-form div.input label { }
.login-box .login-form div.input input { border-radius:5px; padding:3px 8px; background:#fff; border:#cccccc solid 1px; color:#999; font-weight:bold; box-shadow:2px 2px 5px #000, 0px 0px 10px #aaa inset; }
.login-box .login-form div.input input:focus { color:#000; box-shadow:0px 0px 5px #000, 0px 0px 1px #aaa inset; }
{
border-top:#323433 solid 1px;
padding:15px 0 0;
}
.login-box .login-buttons { margin-bottom:15px; }
.login-box .login-buttons input { text-shadow:2px 1px #ce2989; font-weight:bold; box-shadow:2px 2px 5px #000, 0px 0px 10px #ec59bc inset; }
.login-box .login-buttons input:hover { color:#000; box-shadow:0px 0px 5px #000, 0px 0px 1px #ec59bc inset; }
.login-box .login-buttons input:focus { color:#000; box-shadow:0px 0px 3px #000, 0px 0px 1px #ec59bc inset; margin:1px 0px -1px 0px; }
.login-box .login-footer{text-align:center;  border-top: 1px solid #323433; padding:15px 0;}
.login-box .login-footer .apply_button{font-size:18px; box-shadow:0px 0px 5px #000; padding:8px 30px ; display:inline-block; text-decoration:none;}
.login-box .login-footer .apply_button:hover{ color:#000; box-shadow:0px 0px 5px #000, 0px 0px 1px #aaa inset;}
#sidebar {/* float:right; width:225px;*/ }

#test { text-align:left; }
#test div { display:inline-block; text-align:left; padding:50px 10px 50px 50px; }
.table { }
.table th { color: #000000; background:#fefefe; color:#818181; text-align: left; padding:7px 10px; border-bottom:solid 1px #d2d1cb;vertical-align: middle; }
.table td { color: #000000; background:#fbfcfc; border-bottom:solid 1px #e0e0e0; padding:8px 10px;  word-break: break-all; vertical-align: top;}
.table tr.odd td { background:#f8f8f8; }
.table tr:hover td { background:#f0f0f0; }
.table a.ico { }
.box { background:#fbfcfc; height:100%; padding:1px; margin-bottom:20px; float: left; height: 100%; margin: 1%; padding: 1px; width: 45%; }
.box-head { border-bottom: 1px solid #FFFFFF; color: #FFFFFF; height: auto; line-height: 32px; overflow: hidden; padding: 5px 10px; white-space: nowrap; background: rgb(109,122,131); background: url(data:image/svg+xml;base64,); background: -moz-linear-gradient(top, rgba(109, 122, 131, 1) 0%, rgba(69, 86, 96, 1) 100%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(109, 122, 131, 1)), color-stop(100%, rgba(69, 86, 96, 1))); background: -webkit-linear-gradient(top, rgba(109, 122, 131, 1) 0%, rgba(69, 86, 96, 1) 100%); background: -o-linear-gradient(top, rgba(109, 122, 131, 1) 0%, rgba(69, 86, 96, 1) 100%); background: -ms-linear-gradient(top, rgba(109, 122, 131, 1) 0%, rgba(69, 86, 96, 1) 100%); background: linear-gradient(to bottom, rgba(109, 122, 131, 1) 0%, rgba(69, 86, 96, 1) 100%); filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#6d7a83', endColorstr='#455660', GradientType=0 );
}
.box-head .right { padding-top:5px; line-height:13px; }
.box-head .right label, .box-head .right input { float:left; }
.box-head .right label { padding:4px 0 0 0; }
.box-head .right input.field {  height: auto; margin-left:5px; padding: 4px 10px; border:#fff solid 1px; }
.box-head .right input.button { margin-left:0px; padding:0px 10px 1px; border-radius:0px; }
.pagging { height:20px; padding:8px 10px; line-height:19px; color:#949494; }
.pagging a { background:url(../img/pagging.gif) repeat-x 0 0; height:20px; float:left; padding:0 8px; border:solid 1px #d5d5d5; text-decoration: none; color:#949494; margin-left:5px; }
.pagging a:hover { border-color:#8c3521; background:#ba4c32; color:#fff; }
.pagging span { float:left; margin-left:5px; padding-top:2px; }
.box.single { margin:0 auto; width:500px; float:none; }
.box.full { width:auto; float:none; margin-left:0; margin-right:0; }
a.ico { color:#9d9c9a; font-size:10px; margin:0 5px 5px; display:inline-block; text-decoration: none; padding:0 0 0 14px; background-repeat:no-repeat; background-position:0 0; }
a.ico:hover { color:#333; }
a.del { background-image:url(../img/del.gif) }
a.edit { background-image:url(../img/edit.gif); margin-left:14px; }
.size1 { width:716px; }
.size2 { width:80px; }
.size3 { width:120px; }
textarea.size1 { height:180px; }
.buttons { text-align: right; padding:10px; background:#eee; border-top:solid 1px #e0e0e0; }
.buttons .button { margin-left:3px; }
.box-content { padding:10px; }
.sort { border-top:solid 1px #e0e0e0; padding-top:10px; margin-top:15px; }
.sort label { display:block; font-weight: bold; }
.sort .field { display:block; margin-top:5px; }
.sort select.field { width:203px; }
.box-content p { padding-top:10px; }
.select-all { }
.select-all label { text-decoration: underline; padding-left:4px; }
.add-button, .add-button span { float:left; height:26px; background:url(../img/add-button.gif) no-repeat 0 0; padding:0 0 0 28px; }
.add-button span { background-position:right 0; padding:0 10px 0 0; }
.add-button { font-weight: bold; font-size:12px; color:#747474; text-decoration: none; line-height:26px; }
.add-button:hover { color:#333; }
#footer { text-align:center; height:44px; line-height:44px; color:#fff; background: rgb(234,49,191); background: url(data:image/svg+xml;base64,); background: -moz-linear-gradient(top, rgba(234, 49, 191, 1) 0%, rgba(236, 0, 140, 1) 100%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(234, 49, 191, 1)), color-stop(100%, rgba(236, 0, 140, 1))); background: -webkit-linear-gradient(top, rgba(234, 49, 191, 1) 0%, rgba(236, 0, 140, 1) 100%); background: -o-linear-gradient(top, rgba(234, 49, 191, 1) 0%, rgba(236, 0, 140, 1) 100%); background: -ms-linear-gradient(top, rgba(234, 49, 191, 1) 0%, rgba(236, 0, 140, 1) 100%); background: linear-gradient(to bottom, rgba(234, 49, 191, 1) 0%, rgba(236, 0, 140, 1) 100%); filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ea31bf', endColorstr='#ec008c', GradientType=0 );
border-top: 1px solid #FFFFFF; box-shadow: 0 -2px 3px 0 #000000; color: #FFFFFF; height: 44px; line-height: 44px; }
#footer a { color:#fff; }
.msg { position:relative;  margin-bottom:10px; }
.msg p { padding:9px 10px 9px 40px; font-size:12px; background-repeat:no-repeat; background-position:10px 5px; }
.msg a.close { font-size:0; line-height:0; position:absolute; top:0; right:0; width:34px; height:34px; background:url(../img/close.gif); text-indent: -4000px; }
.msg-ok p { background-color:#E5D0DD; border:solid 1px #E61597; color:#000; background-image:url(../img/msg-ok.png); }
.msg-error p { border:solid 1px #E61597; color:#000; background:url(../img/msg-error.png) 5px 5px no-repeat #E5D0DD;     margin: 0 0 5px;}
.error-message { color:#f00; }
/*** CSS3 ***/
.box { -moz-border-radius:5px; -webkit-border-radius:5px; -moz-box-shadow: 0 0 6px #978f6c; -webkit-box-shadow: 0 0 6px #978f6c; }
.box-head { -moz-border-radius-topleft:5px; -moz-border-radius-topright:5px; -webkit-border-top-left-radius:5px; -webkit-border-top-right-radius:5px; }
.field, .button { -moz-border-radius:4px; -webkit-border-radius:4px; }
.small-field, .button, .pagging a { -moz-border-radius:3px; -webkit-border-radius:3px; }
.msg p { -moz-border-radius:6px; -webkit-border-radius:6px; }
.button { -moz-box-shadow: 0 0 2px #978f6c; -webkit-box-shadow: 0 0 2px #978f6c; }
.form { padding:15px 10px 5px 10px; }
.form label { display: block; float: left; font-size: 13px; font-weight: bold; margin: 0 15px 0 0; padding-bottom: 4px; }
.form label span { color:#999; font-weight: normal; }
.form .field { display:block; }
.form .inline-field .field { display:inline; }
.form .req { font-size:13px; float:right; white-space:nowrap; color:#999; }
.form p { overflow:hidden; padding-bottom:15px; }
.form div.input { clear: both; overflow: hidden; padding: 5px 0; width: 275px; margin: 0 auto 5px; }
.form .fields { overflow:hidden; }
.form .fields li { list-style:none; min-height: 52px; width:50%; float:left; margin:0 0 10px; }
.form .fields li.gallery-data { font-size:13px; width:100%; vertical-align:middle; line-height:1.5em; font:arial; }
.form .fields li label { width:30%; float:left; }
.form .fields li label ~ label{ width:auto; float:none; display:inline;padding:0;}
.form .fields li label span { color:#999; font-weight: normal; display:block; }
.form .fields li input[type="text"] { border:1px solid #7F9DB9; }
.form .fields li input[type="password"] { border:1px solid #7F9DB9; }
.form .fields li input[type="radio"] { margin:0 5px 0px 0; vertical-align:baseline; }
.form .fields li input[type="checkbox"] { margin:0 5px 0px 0; vertical-align:baseline; }
.form .fields li .weekdays {overflow:hidden;}
.form .fields li .weekdays label{width:28%; float:left; padding:0;}
.form .list {overflow:hidden;padding:0 10px;}
.form .list li{ list-style:none; margin:0 0 10px;}
.form .list li.download-link{float:right; width:auto;}
.columns-3 .fields { overflow:hidden; padding:0px 0 0 25px; }
.columns-3 .fields li { list-style:none; min-height: 50px; width:300px; float:left; margin:0 20px 10px 0; }
.columns-3 .fields li.gallery-data { font-size:13px; vertical-align:middle; line-height:1.5em; font:arial; }
.columns-3 .fields li.gallery-data .gallery_image{margin:0 0 10px;height: auto;margin: 0 0 10px;width: 100% ;}
.columns-3 .fields li.gallery-data img{vertical-align:middle;}
.columns-3 .fields li.gallery-data .action-buttons{float:right;}
.columns-3 .fields li label { width:30%; float:left; }
.columns-3 .fields li label span { color:#999; font-weight: normal; display:block; }
.columns-3 .fields li input[type="text"] { border:1px solid #7F9DB9; }
.columns-3 .fields li input[type="password"] { border:1px solid #7F9DB9; }
.sql { color:#fff; }
